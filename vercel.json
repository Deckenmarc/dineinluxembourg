{
  "buildCommand": "./build.sh",
  "devCommand": "bun run dev",
  "framework": "nextjs",
  "outputDirectory": ".vercel/output",
  "git": {
    "deploymentEnabled": {
      "master": true
    }
  },
  "ignoreCommand": "git diff HEAD^ HEAD --quiet ./",
  "version": 2,
  "env": {
    "NEXT_TYPESCRIPT_IGNORE_BUILD_ERRORS": "1",
    "NEXT_ESLINT_IGNORE_BUILD_ERRORS": "1"
  },
  "regions": ["iad1"],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
} 